{
    "version": "2.0.0",
    "tasks": [
      {
        "type": "shell",
        "label": "gcc build active file",
        "command": "/usr/bin/gcc",
        "args": ["-g","-W", "-Wall","-Wextra", "${file}", "-o", "${fileDirname}/${fileBasenameNoExtension}", ],
        "options": {
          "cwd": "/usr/bin"
        },
        "problemMatcher": {
          "base": "$tsc",
          "fileLocation": ["absolute"]
      },
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }